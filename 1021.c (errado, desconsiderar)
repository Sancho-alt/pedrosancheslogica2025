#include <stdio.h>

int main()
{
    char C [7];
    C[0] = 100;
    C[1] = 50;
    C[2] = 20;
    C[3] = 10;
    C[4] = 5;
    C[5] = 2;
    char Q [7];
    int N, I, R, A, X, T, Alt;
    double M, Y, U;
    scanf("%lf", &M);
    printf("NOTAS:\n");
    N = M;
    Alt = N;
    for(I = 0; I < 6; I++){
        R = N % C[I];
        A = N - R;
        X = A / C[I];
        Q[I] = X;
        N = R;
        printf("%d nota(s) de R$ %d.00\n", Q[I], C[I]);
    }
    M = M - Alt + N;
    T = M * 100;
    char W [7];
    W[0] = 100;
    W[1] = 50;
    W[2] = 25;
    W[3] = 10;
    W[4] = 5;
    W[5] = 1;
    char P[7];
    printf("MOEDAS:\n");
    for(I = 0; I < 6; I++){
        R = T % W[I];
        A = T - R;
        X = A / W[I];
        P[I] = X;
        T = R;
        U = W[I];
        Y = U / 100;
        printf("%d moeda(s) de R$ %.2lf\n", P[I], Y);
    }
    

    return 0;
}
